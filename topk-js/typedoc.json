{
  "entryPoints": ["index.d.ts"],
  "entryPointStrategy": "resolve",
  "tsconfig": "tsconfig.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter", "./typedoc/custom-frontmatter-plugin.mjs"],
  "readme": "none",
  "out": "../docs/api",
  "router": "module",
  "groupOrder": [
    "Variables", "Functions", "*"
  ],
  "entryFileName": "index",
  "fileExtension": ".mdx",
  "hidePageHeader": true,
  "hidePageTitle": true,
  "parametersFormat": "table",
  "propertyMembersFormat": "table",
  "interfacePropertiesFormat": "table",
  "classPropertiesFormat": "table",
  "frontmatterCommentTags": ["title"],
  "useCodeBlocks": true,
  "indexFrontmatter": {
    "title": "topk-js"
  }
}
