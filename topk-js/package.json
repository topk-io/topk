{
  "name": "topk-js",
  "version": "0.4.5",
  "napi": {
    "binaryName": "topk-js",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "armv7-unknown-linux-musleabihf",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-linux-gnu",
        "i686-pc-windows-msvc",
        "x86_64-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin",
        "riscv64gc-unknown-linux-gnu"
      ]
    }
  },
  "files": [
    "lib",
    "index.js",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./schema": {
      "require": "./lib/schema.js",
      "import": "./lib/schema.js",
      "types": "./lib/schema.d.ts"
    },
    "./query": {
      "require": "./lib/query.js",
      "import": "./lib/query.js",
      "types": "./lib/query.d.ts"
    },
    "./data": {
      "require": "./lib/data.js",
      "import": "./lib/data.js",
      "types": "./lib/data.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "schema": [
        "lib/schema.d.ts"
      ],
      "query": [
        "lib/query.d.ts"
      ],
      "data": [
        "lib/data.d.ts"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@napi-rs/cli": "3.0.0-alpha.75",
    "@sindresorhus/tsconfig": "^7.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --no-const-enum --platform --release",
    "dev": "napi build --no-const-enum --platform && tsc --noEmit",
    "build:debug": "napi build --no-const-enum --platform",
    "prepublishOnly": "napi prepublish -t npm --no-gh-release",
    "typecheck": "tsc --noEmit",
    "test": "jest --verbose",
    "universal": "napi universalize",
    "version": "napi version"
  }
}
